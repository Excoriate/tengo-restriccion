{
  "name": "tengo-restriccion",
  "version": "1.0.0",
  "description": "App para notificacion de restriccion vehicular segun patente",
  "scripts": {
    "start": "parallelshell \"NODE_ENV=development nodemon index.js\"  \"node webpack-server.js\" \"npm run watch\"",
    "start_win": "parallelshell \"set NODE_ENV=development&&node index.js\"  \"node webpack-server.js\" \"npm run watch\"",
    "production": "NODE_ENV=production nodemon index.js",
    "test": "eslint . && NODE_ENV=test mocha --compilers js:babel/register --reporter spec 'app/**/__tests__/*-test.js'",
    "build": "NODE_ENV=production npm run build:bundle",
    "build:bundle": "webpack -p --config webpack.config.production.js",
    "build:css": "node-sass app/public/scss/screen.scss -o app/public/css/ --source-map app/public/scss/screen.css.map && postcss --use autoprefixer -d app/public/css/ app/public/css/screen.css",
    "watch": "onchange \"app/public/scss/**/*.scss\" -- npm run build:css"
  },
  "author": "SantiagoLab.cl <contact@santiagolab.cl>",
  "license": "MIT",
  "dependencies": {
    "bunyan": "^1.4.0",
    "chai": "^3.0.0",
    "chai-as-promised": "^5.1.0",
    "chai-things": "^0.2.0",
    "cheerio": "^0.19.0",
    "koa": "^0.21.0",
    "koa-body": "^1.2.1",
    "koa-route": "^2.4.2",
    "mandrill-api": "^1.0.45",
    "mocha": "^2.2.5",
    "moment": "^2.10.3",
    "mongoose": "^4.0.7",
    "parallelshell": "^1.2.0",
    "ramda": "^0.15.1",
    "react": "^0.14.0-beta1",
    "react-dom": "^0.14.0-beta1",
    "request": "^2.58.0"
  },
  "devDependencies": {
    "autoprefixer": "^5.2.0",
    "babel": "^5.6.14",
    "babel-core": "^5.6.18",
    "babel-loader": "^5.3.1",
    "eslint": "^0.24.1",
    "eslint-plugin-react": "^2.7.0",
    "koa-static": "^1.4.9",
    "node-libs-browser": "^0.5.2",
    "node-sass": "^3.2.0",
    "onchange": "^1.1.0",
    "postcss-cli": "^1.3.1",
    "react-hot-loader": "^1.2.8",
    "webpack": "^1.10.1",
    "webpack-dev-server": "^1.10.1",
    "supertest": "^1.0.1",
    "supertest-as-promised": "^2.0.2"
  }
}
